<p>Found User: <%= @user.username %></p>

<%= form_for(@user) do |f| %>
  <%= f.label :username, 'Your Username Here' %>
  <br>
  <%= f.text_field 'username' %>
  <br>
  <br>
  <%= f.label 'Email' %>
  <br>
  <%= f.text_field :email %>
  <br>
  <br>
  <%= f.label 'Password' %>
  <br>
  <%= f.password_field 'password' %>
  <br>
  <br>
  <%= f.submit 'Submit' %>
<% end %>